{
  "productName": "app-starter",
  "version": "0.0.0",
  "identifier": "io.devdept.app",
  "build": {
    "devUrl": "http://localhost:1420",
    "frontendDist": "../../../dist/apps",
    "beforeDevCommand": "vite",
    "beforeBuildCommand": "tsc && vite build"
  },
  "app": {
    "windows": [
      {
        "title": "app",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
