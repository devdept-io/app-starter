{
  "productName": "app",
  "version": "0.0.0",
  "identifier": "com.starter.dev",
  "build": {
    "devUrl": "http://localhost:1420",
    "frontendDist": "../../../dist/apps",
    "beforeDevCommand": "nx serve app",
    "beforeBuildCommand": "cd ../../../ nx build app"
  },
  "app": {"windows": [
      {
        "title": "app",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
